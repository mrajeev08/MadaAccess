#!/bin/bash
#SBATCH --ntasks=39
#SBATCH --cpus-per-task=1
#SBATCH -J  0102_aggregateWP
#SBATCH -t 12:00:00
# sends mail when process begins, and
# when it ends
#SBATCH --mail-type=begin
